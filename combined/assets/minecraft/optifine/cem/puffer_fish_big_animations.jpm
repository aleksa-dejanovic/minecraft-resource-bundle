{
	"credit": "Created by FreshLX for Fresh Animations",
	"animations": [
		{
			"varb.fcc": "frame_counter == var.frame_counter_prev",
			"var.hy": "clamp(head_yaw,-90,90) * clamp( 1-abs( var.position-pos_y )*2, 0, 1 )",
			"var.r": "random(id)*pi*4",
			"var.up": "var.up +abs( var.pre_posy-pos_y )",
			"var.ls": "var.r +limb_swing*1.6 +age/6 +var.up*4",
			"var.ft": "limb_swing/1.5 +age/3",
			"var.m": "clamp( limb_speed*3 +abs( var.position-pos_y ), 0, 1 )",
			"var.waves": "pos_x*1.5 +pos_y*2 +pos_z*1.5 +age/17",
			"var.water": "clamp( if( varb.fcc, var.water, is_in_water, var.water +1 *frame_time, var.water -2 *frame_time ), 0, 1 )",
			"var.position": "if( varb.fcc, var.position, age<9, pos_y, pos_y*min(1,frame_time*6) +var.position*max(0,1-frame_time*6) )",
			"var.Created_by_FreshLX_for_Fresh_Animations": "0"
		},
		{
			"body.ty": "22",
			"body2.ry": "( sin(var.r+limb_swing  +age/12)/5 +torad(var.hy)/2                                )*var.water   -sin(limb_swing+age/1.6)/4*(1-var.water)",
			"body2.rz": "( sin(var.r+limb_swing/2+age/14)/5                                                 )*var.water +(-cos(limb_swing+age/1.8)/6 +sin( (limb_swing/1.5 +age/3)/6 +sin( (limb_swing/1.5 +age/3)/3 )*1.2 )*pi/2 )*(1-var.water)",
			"body2.rx": "( sin(var.r+limb_swing/2+age/16)/5 +torad(-15) +torad(40)*clamp(limb_speed*2, 0, 1))*var.water   -sin(limb_swing+age/2  )/4*(1-var.water) +clamp( ( var.position-pos_y )*1.5, -1.5, 1.5 )",
			"body2.tx": "( cos(var.r+limb_swing  +age/12)/3                                                 )*var.water   -cos(limb_swing+age/1.6)  *(1-var.water)",
			"body2.ty": "-3.5",
			"body2.sx": "1 +(0.5-random(id))/3 +sin(var.r+age/1.5 -cos(var.r+age/1.5)/2)/20",
			"body2.sy": "body2.sx",
			"body2.sz": "body2.sx"
		},
		{
			"left_fin.rx": " torad( 20)",
			"right_fin.rx": "torad( 20)",
			"left_fin.ry": " torad( 5 ) +(  cos(torad(30)+var.r+limb_swing/2+age/1.3)/3*clamp(1-limb_speed*6, 0.3, 1) )*var.water",
			"right_fin.ry": "torad(-5 ) +( -sin(torad(30)+var.r+limb_swing/2+age/1.2)/3*clamp(1-limb_speed*6, 0.3, 1) )*var.water",
			"left_fin.rz": " torad(-15) +(  sin(          var.r+limb_swing/2+age/1.3)/2*clamp(1-limb_speed*6, 0.3, 1) +(torad(-10) -sin(var.r+limb_swing*1.6+age/6)/3)*clamp(limb_speed*6, 0, 1) )*var.water +sin(age)*(1-var.water)",
			"right_fin.rz": "torad( 15) +(  cos(          var.r+limb_swing/2+age/1.2)/2*clamp(1-limb_speed*6, 0.3, 1) +(torad( 10) -sin(var.r+limb_swing*1.6+age/6)/3)*clamp(limb_speed*6, 0, 1) )*var.water -sin(age)*(1-var.water)",
			"left_fin.sx": " 2-body2.sx",
			"right_fin.sx": "2-body2.sx",
			"left_fin.sy": "left_fin.sx",
			"right_fin.sy": "right_fin.sx",
			"left_fin.sz": "left_fin.sx",
			"right_fin.sz": "right_fin.sx"
		},
		{
			"var.pre_posy": "pos_y",
			"var.frame_counter_prev": "frame_counter"
		}
	]
}