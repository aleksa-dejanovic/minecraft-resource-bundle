{
	"credit": "Created by FreshLX for Fresh Animations",
	"animations": [
		{
			"varb.fcc": "frame_counter == var.frame_counter_prev",
			"var.hy": "clamp(head_yaw,-90,90) * clamp( 1-abs( var.position-pos_y )*2, 0, 1 )",
			"var.r": "random(id)*pi*4",
			"var.up": "var.up +abs( var.pre_posy-pos_y )",
			"var.ls": "var.r +limb_swing*2 +age/7 +var.up*4",
			"var.ft": "limb_swing/3 +age/3",
			"var.m": "clamp( limb_speed*3 +abs( var.position-pos_y ), 0, 1 )",
			"var.waves": "pos_x*1.5 +pos_y*2 +pos_z*1.5 +age/17",
			"var.water": "clamp( if( varb.fcc, var.water, is_in_water, var.water +1 *frame_time, var.water -2 *frame_time ), 0, 1 )",
			"var.position": "if( varb.fcc, var.position, age<9, pos_y, pos_y*min(1,frame_time*6) +var.position*max(0,1-frame_time*6) )"
		},
		{
			"body.ry": "-tail.ry/6  +sin( var.ft/3 +sin( var.ft/5 )/2 )*(1-var.water)",
			"body.rz": "if( is_in_water, 0, -pi/2 )",
			"body.rx": "( clamp( ( var.position-pos_y )*3, -1.5, 1.5 ) -sin( var.waves )/24*(1-var.m) )*var.water",
			"body2.rz": "( -cos( var.ls )/10*var.m -cos( var.waves )/20*(1-var.m) )*var.water +( sin( var.ft/6 +sin( var.ft/3 )*1.2 )*pi/2 )*(1-var.water)",
			"body2.ty": "if( is_in_water, -1.5,-5) +cos( var.waves )               *var.water +(                     clamp( ( var.position-pos_y )*4,-1,1)*4 +3 )*clamp(1-var.water*4,0,1)",
			"body2.ry": "   (  sin( var.ls )/13*(0.5+var.m*1.5) +torad(var.hy)/2  )*var.water +( -cos( var.ft )/4   +clamp( ( var.position-pos_y )*2,-1,1)*body2.rz   )*(1-var.water)",
			"tail2.tx": "   (  sin( var.ls )/6 *(0.5+var.m*1.5) +torad(var.hy)/4  )*var.water +( -sin( var.ft )/6   -clamp( ( var.position-pos_y )*2,-1,1)*body2.rz/2 )*(1-var.water)",
			"tail2.ry": "   ( -cos( var.ls )/3 *(0.5+var.m*1.5) +torad(var.hy)/1.5)*var.water +( -sin( var.ft )/1.5 -clamp( ( var.position-pos_y )*5,-1,1)*body2.rz   )*(1-var.water)",
			"tail2.rz": "   (  cos( 0.8 +var.ls )/6 *(1 -var.m)                   )*var.water    -sin( var.ft )/3                                                      *(1-var.water)",
			"fin_top2.rz": "(  cos( var.ls )/8 *(1  -var.m*0.5)                   )*var.water +(  sin( var.ft )/1.5 -clamp( ( var.position-pos_y )*5,-1,1)*body2.rz   )*(1-var.water)",
			"body2.tx": "   (  cos( var.ls )/5 *(0.5+var.m*1.5)                   )*var.water    -cos( var.ft )   *(1-var.water)   +if( is_in_water, 0, -1.82 )",
			"tail2.rx": "   ( -sin( var.waves )/10*(1-var.m) -max(0,1-var.m*2)/20 )*var.water    -sin( var.ft )/6 *(1-var.water)",
			"body2.tz": "3",
			"fin_bottom2.rz": "-fin_top2.rz/1.5",
			"body2.sx": "1 +( 0.5-random(id) )/3",
			"body2.sy": "body2.sx",
			"body2.sz": "body2.sx"
		},
		{
			"left_fin.rx": " torad(-20)",
			"right_fin.rx": "torad(-20)",
			"left_fin.ry": " torad(-35          ) +(  cos(      var.r +limb_swing/2 +age/4 +sin( var.r +limb_swing/2 +age/8 )   )/2 *max(0,1-var.m*2) +( -0.5 -sin( var.r +limb_swing*1.5 +age/6 )/3 ) *min(1,var.m*2) )*var.water +sin(age/1.5)/2*(1-var.water)",
			"right_fin.ry": "torad( 35          ) +( -sin(      var.r +limb_swing/2 +age/4 +cos( var.r +limb_swing/2 +age/8 )   )/2 *max(0,1-var.m*2) +(  0.5 -sin( var.r +limb_swing*1.5 +age/6 )/3 ) *min(1,var.m*2) )*var.water -sin(age/1.5)/2*(1-var.water)",
			"left_fin.rz": " torad( 10 +9*var.m ) +( -cos( 0.5 +var.r +limb_swing/2 +age/4 +sin( var.r +limb_swing/2 +age/8 )/2 )/4 *max(0,1-var.m*2) )*var.water",
			"right_fin.rz": "torad(-10 -9*var.m ) +(  sin( 0.5 +var.r +limb_swing/2 +age/4 +cos( var.r +limb_swing/2 +age/8 )/2 )/4 *max(0,1-var.m*2) )*var.water"
		},
		{
			"var.pre_posy": "pos_y",
			"var.frame_counter_prev": "frame_counter"
		}
	]
}