{
  "model": {
    "type": "minecraft:condition",
        "property": "minecraft:has_component",
        "component": "minecraft:enchantments",
        "ignore_default": true,
    "on_true": {
      "type": "minecraft:composite",
      "models": [
        {
          "type": "minecraft:range_dispatch",
          "property": "minecraft:damage",
          "entries": [
            {
              "threshold": 0,
              "model": {
                "type": "minecraft:model",
                "model": "minecraft:item/shears"
              }
            },
            {
              "threshold": 0.85,
              "model": {
                "type": "minecraft:model",
                "model": "minecraft:item/shears_damaged0"
              }
            },
            {
              "threshold": 0.9,
              "model": {
                "type": "minecraft:model",
                "model": "minecraft:item/shears_damaged1"
              }
            },
            {
              "threshold": 0.95,
              "model": {
                "type": "minecraft:model",
                "model": "minecraft:item/shears_damaged2"
              }
            }
          ]
        },
        {
          "type": "minecraft:range_dispatch",
          "property": "minecraft:damage",
          "entries": [
            {
              "threshold": 0,
              "model": {
                "type": "minecraft:model",
                "model": "minecraft:item/misc_outlines/enchanted_shears"
              }
            },
            {
              "threshold": 0.85,
              "model": {
                "type": "minecraft:model",
                "model": "minecraft:item/diminishing/misc_outlines/enchanted_shears_02"
              }
            },
            {
              "threshold": 0.9,
              "model": {
                "type": "minecraft:model",
                "model": "minecraft:item/diminishing/misc_outlines/enchanted_shears_03"
              }
            },
            {
              "threshold": 0.95,
              "model": {
                "type": "minecraft:model",
                "model": "minecraft:item/diminishing/misc_outlines/enchanted_shears_04"
              }
            }
          ]
        }
      ]
    },
    "on_false": {
      "type": "minecraft:condition",
          "property": "minecraft:has_component",
          "component": "minecraft:enchantment_glint_override",
          "ignore_default": true,
      "on_true": {
        "type": "minecraft:composite",
        "models": [
          {
            "type": "minecraft:range_dispatch",
            "property": "minecraft:damage",
            "entries": [
              {
                "threshold": 0,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/shears"
                }
              },
              {
                "threshold": 0.85,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/shears_damaged0"
                }
              },
              {
                "threshold": 0.9,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/shears_damaged1"
                }
              },
              {
                "threshold": 0.95,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/shears_damaged2"
                }
              }
            ]
          },
          {
            "type": "minecraft:range_dispatch",
            "property": "minecraft:damage",
            "entries": [
              {
                "threshold": 0,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/misc_outlines/enchanted_shears"
                }
              },
              {
                "threshold": 0.85,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/diminishing/misc_outlines/enchanted_shears_02"
                }
              },
              {
                "threshold": 0.9,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/diminishing/misc_outlines/enchanted_shears_03"
                }
              },
              {
                "threshold": 0.95,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/diminishing/misc_outlines/enchanted_shears_04"
                }
              }
            ]
          }
        ]
      },
      "on_false": {
        "type": "minecraft:range_dispatch",
        "property": "minecraft:damage",
        "entries": [
          {
            "threshold": 0,
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/shears"
            }
          },
          {
            "threshold": 0.85,
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/shears_damaged0"
            }
          },
          {
            "threshold": 0.9,
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/shears_damaged1"
            }
          },
          {
            "threshold": 0.95,
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/shears_damaged2"
            }
          }
        ]
      }
    }
  }
}