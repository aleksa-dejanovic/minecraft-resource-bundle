            },
            "when": "minecraft:redstone"
          },
          {
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/golden_helmet_copper_trim"
            },
            "when": "minecraft:copper"
          },
          {
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/golden_helmet_gold_trim"
            },
            "when": "minecraft:gold"
          },
          {
            "model": {
              "type": "minecraft:model",
